<script type="text/javascript" src="http://code.jquery.com/jquery-1.11.0.min.js"></script>   // above </head> //

  
<script type="text/javascript">        // above </body> //     
$(document).ready(function(){
// this part disables the right click
$('img').on('contextmenu', function(e) {
return false;
});
//this part disables dragging of image
$('img').on('dragstart', function(e) {
return false;
});

});
</script>
